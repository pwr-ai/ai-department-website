@article{kazienko1998struktura,
 abstract = {Idea hipertekstu została po raz pierwszy przedstawiona przez Vannevar Busha w 1945 roku [10]. Jednakże dynamiczny rozwój koncepcji hipertekstu nastąpił dopiero w latach osiemdziesiątych i dziewięćdziesiątych. Opracowano wtedy wiele modeli hipertekstu, jak np. Dexter [27], HDM [24], Proxhy [30], Microcosm [28] czy bardziej formalny-XHMBS [38]; nadal prowadzone są badania w tym zakresie. Napisanych zostało także dużo książek i artykułów w czasopismach naukowych, jak również cyklicznie odbywają się wielkie międzynarodowe},
 author = {Kazienko, Przemys{\l}aw and Informacyjnych, Zak{\l}ad System{\'o}w},
 journal = {Zagadnienia Informacji Naukowej},
 number = {2},
 pages = {72},
 pub_year = {1998},
 title = {Struktura hipertekstu a struktura systemu WWW},
 venue = {Zagadnienia Informacji Naukowej}
}

