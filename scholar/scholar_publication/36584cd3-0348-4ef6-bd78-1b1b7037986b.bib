 author = {Maziarz, Marek and Piasecki, Maciej and Rudnicka, Ewa and Szpakowicz, Stan},
 booktitle = {Proceedings of the International Conference Recent Advances in Natural Language Processing RANLP 2013},
 pages = {443--452},
 pub_year = {2013},
 title = {Beyond the transfer-and-merge wordnet construction: plWordNet and a comparison with WordNet},
