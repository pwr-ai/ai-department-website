 author = {Augustyniak, {\L}ukasz and Rajda, Krzysztof and Kajdanowicz, Tomasz and Bernaczyk, Micha{\l}},
 journal = {arXiv preprint arXiv:2006.10207},
 pub_year = {2020},
 title = {Political Advertising Dataset: the use case of the Polish 2020 Presidential Elections},
