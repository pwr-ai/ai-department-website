@article{piasecki2008towards,
 abstract = {A recently started project on building the Polish wordnet (called plWordNet) is the background to the presented work. The goals of the project and the linguistic structure of plWordNet are described. The assumptions of the planned semi-automatic method of extending the initial core wordnet to its full size are presented. A multi-criteria algorithm of extraction of lexical semantic relations is presented. Several problems with application of the known methods of relation extraction to Polish–an inflectional and free word order language},
 author = {Piasecki, Maciej},
 journal = {International Journal of Intelligent Information and Database Systems},
 number = {3},
 pages = {336--353},
 pub_year = {2008},
 publisher = {Inderscience Publishers},
 title = {Towards semi-automatic extraction of lexical semantics relations for Polish},
 venue = {… Journal of Intelligent Information and Database …},
 volume = {2}
}

