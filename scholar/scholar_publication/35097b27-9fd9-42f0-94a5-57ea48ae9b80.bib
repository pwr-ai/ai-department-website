@article{piasecki1998modelowanie,
 abstract = {Przedstawiono nową metodę budowania nawigacyjnej mapy nieznanego otoczenia, wykorzystującą różnicową reprezentację wiedzy zdobywanej w czasie działania robota (pomiarów pozycji zliczonej i obserwacji otoczenia za pomocą sensorów). Metoda ta jest szczególnie użyteczna w początkowych etapach budowania mapy, kiedy ogromna wiedza ma charakter fragmentaryczny a niedokładności pomiarów generują niespójności lub sprzeczności w możliwych rozwiązaniach.},
 author = {Piasecki, M},
 journal = {Pomiary Automatyka Robotyka},
 number = {7-8},
 pages = {16--20},
 pub_year = {1998},
 title = {Modelowanie otoczenia robota mobilnego za pomoc{\k{a}} map r{\'o}{\.z}nicowych},
 venue = {Pomiary Automatyka Robotyka},
 volume = {2}
}

