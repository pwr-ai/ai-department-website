@article{kobylinski2019poleval,
 abstract = {abstrakt w j. angielskim: PolEval is a SemEval-inspired evaluation campaign for natural language processing tools for Polish. Submitted tools compete againstone another within certain tasks selected by organizers, using available data and are evaluated according to pre-established procedures. It is organized since 2017 and each year the winning systems become the state-of-the-art in Polish language processing in the respectivetasks. In 2019 we have organized six different tasks, creating an even greater opportunity for NLP researchers},
 author = {Kobyli{\'n}ski, {\L}ukasz and Ogrodniczuk, Maciej and Koco{\'n}, Jan and Marci{\'n}czuk, Micha{\l} and Smywi{\'n}ski-Pohl, Aleksander and Wo{\l}k, Krzysztof and Kor{\v{z}}inek, Danijel and Ptaszy{\'n}ski, Micha{\l} and Pieciukiewicz, Agata and Dyba{\l}a, Pawe{\l}},
 pub_year = {2019},
 publisher = {Pozna{\'n}: Nauka i Innowacje},
 title = {PolEval 2019: the next chapter in evaluating Natural Language Processing tools for Polish},
 venue = {NA}
}

