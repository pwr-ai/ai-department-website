@article{kocon2018kpwr,
 abstract = {W zadaniu interpretacji wyrażeń temporalnych celem jest uzyskanie znaczenia wyrażenia temporalnego w kontekście. Od strony informatycznej mówi się tutaj o zadaniu normalizacji wyrażeń temporalnych, czyli sprowadzaniu wyrażenia do jednoznacznej postaci rozumianej przez komputer. Przyjęto rozwiązanie, które uwzględnia zarówno lokalne jak i globalne znaczenie wyrażenia temporalnego. Znaczenie lokalne jest wynikiem analizy wyrażenia temporalnego bez uwzględnienia kontekstu, w jakim wyrażenie występuje. Tylko tokeny},
 author = {Koco{\'n}, Jan},
 pub_year = {2018},
 publisher = {Wroc{\l}aw University of Science and Technology},
 title = {KPWr annotation guidelines-normalisation of temporal expressions},
 venue = {NA}
}

