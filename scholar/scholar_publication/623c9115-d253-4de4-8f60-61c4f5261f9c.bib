@article{burdka2014pytania,
 abstract = {Description Zbiór wzbogacono o oznaczenie konkretnych fragmentów zawierających odpowiedź na wskazane pytania. Wszystkie wskazane fragmenty zostały zweryfikowane przez człowieka. Niektórym pytaniom przypisano wiele fragmentów. Celem zbioru jest testowanie i rozwój systemów typu Question Answering dla języka polskiego. Liczymy na to, że dzięki udostępnieniu zarówno pytań, jak i całej kolekcji testowej (całej Wikipedii z wybranego zrzutu), możliwe będzie przeprowadzenie różnych eksperymentów w tych},
 author = {Burdka, {\L}ukasz and Hry{\'n}czak, Justyna and Jakubaszek, Maja and Juros, Agata and Marci{\'n}czuk, Micha{\l} and Piasecki, Dominik and Piasecki, Maciej and Pilarska, Aleksandra and Ptak, Marcin and Radziszewski, Adam and others},
 pub_year = {2014},
 publisher = {Wroc{\l}aw University of Science and Technology},
 title = {Pytania i odpowiedzi z serwisu wikipedyjnego" Czy wiesz", wersja 2.0},
 venue = {NA}
}

