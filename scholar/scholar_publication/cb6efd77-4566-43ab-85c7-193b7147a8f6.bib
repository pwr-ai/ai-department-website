 author = {K{\k{e}}dzia, Pawe{\l} and Piasecki, Maciej and Orli{\'n}ska, Marlena},
 journal = {Cognitive Studies},
 number = {15},
 pub_year = {2015},
 publisher = {Instytut Slawistyki Polskiej Akademii Nauk},
 title = {Word sense disambiguation based on large scale Polish CLARIN heterogeneous lexical resources},
 venue = {Cognitive Studies}
}

