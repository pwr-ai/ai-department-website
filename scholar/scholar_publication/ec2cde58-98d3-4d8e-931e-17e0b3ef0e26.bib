@inproceedings{kocon2019multi,
 abstract = {In this article we present an extended version of PolEmo–a corpus of consumer reviews from 4 domains: medicine, hotels, products and school. Current version (PolEmo 2.0) contains 8,216 reviews having 57,466 sentences. Each text and sentence was manually annotated with sentiment in 2+ 1 scheme, which gives a total of 197,046 annotations. We obtained a high value of Positive Specific Agreement, which is 0.91 for texts and 0.88 for sentences. PolEmo 2.0 is publicly available under a Creative Commons copyright license. We explored},
 author = {Koco{\'n}, Jan and Mi{\l}kowski, Piotr and Za{\'s}ko-Zieli{\'n}ska, Monika},
 booktitle = {Proceedings of the 23rd Conference on Computational Natural Language Learning (CoNLL)},
 pages = {980--991},
 pub_year = {2019},
 title = {Multi-level sentiment analysis of PolEmo 2.0: Extended corpus of multi-domain consumer reviews},
 venue = {Proceedings of the 23rd …}
}

