@article{oleksy2019polish,
 abstract = {Description KPWr (Polish Corpus of Wrocław University of Technology, pol. Korpus Języka Polskiego Politechniki Wrocławskiej) is a corpus of written and spoken documents available on the Creative Common license. The texts are divided into 15 subcorpuses (blogs, science, stenographic recordings, etc.). The documents are annotated on the level of chunks and selected predicate-argument relations, named entities, relations between named entities, anaphora relations, word senses, events, temporal expressions, spatial relations between},
 author = {Oleksy, Marcin and Marci{\'n}czuk, Micha{\l} and Maziarz, Marek and Berna{\'s}, Tomasz and Wieczorek, Jan and Turek, Agnieszka and Fikus, Dominika and Wolski, Micha{\l} and Pustowaruk, Marek and Koco{\'n}, Jan and others},
 pub_year = {2019},
 publisher = {Wroc{\l}aw University of Science and Technology},
 title = {Polish Corpus of Wroc{\l}aw University of Technology 1.3},
 venue = {NA}
}

