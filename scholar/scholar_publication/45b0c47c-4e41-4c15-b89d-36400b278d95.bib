@inproceedings{kazienko2003rosa,
 abstract = {Automatic and non-invasive web personalization seems to be a challenge for nowadays web sites. Many web mining techniques are used to achieve this goal. Since current web sites evolve constantly, web mining operations should be periodically repeated. A multi-agent architecture facilitates integration of different mining methods and permits the discovered knowledge to be verified and updated automatically. We propose ROSA (Remote Open Site Agents)—a system that may be easily incorporated into an existing web},
 author = {Kazienko, Przemys{\l}aw and Kiewra, Maciej},
 booktitle = {International Atlantic Web Intelligence Conference},
 organization = {Springer},
 pages = {297--306},
 pub_year = {2003},
 title = {ROSA—Multi-agent system for web services personalization},
 venue = {International Atlantic Web Intelligence Conference}
}

