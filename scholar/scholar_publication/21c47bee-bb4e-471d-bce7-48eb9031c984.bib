 pub_year = {2003},
 title = {Problemy dost{\k{e}}pu do danych z urz{\k{a}}dze{\'n} typu palmtop z transmisj{\k{a}} danych GPRS/WLAN na przyk{\l}adzie medycznego systemu informacyjnego},
 venue = {Zeszyty Naukowe Wydziału ETI Politechniki Gdańskiej …}
