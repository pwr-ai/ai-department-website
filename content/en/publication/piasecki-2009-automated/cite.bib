 author = {Piasecki, Maciej},
 journal = {Representing Semantics in Digital Lexicography. Innovative Solutions for Lexical Entry Content in Slavic Lexicography},
 pages = {32--43},
 pub_year = {2009},
 title = {Automated extraction of lexical meanings from corpus: A case study of potentialities and limitations},
 venue = {… in Digital Lexicography. Innovative Solutions for …}
}

