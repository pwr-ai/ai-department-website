 author = {Oleksy, Marcin and Koco\ŉ, J and Maryl, M and Piasecki, M},
 booktitle = {Practical Applications of Linguistic Corpora Conference},
 pub_year = {2014},
 title = {Linguistic analysis of weblog genres},
 venue = {Practical Applications of …}
}

