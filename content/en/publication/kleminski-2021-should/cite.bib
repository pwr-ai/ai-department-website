 author = {Klemi\ŉski, Rajmund and Kazienko, Przemyslaw and Kajdanowicz, Tomasz},
 journal = {Journal of Informetrics},
 number = {3},
 pages = {101200},
 pub_year = {2021},
 publisher = {Elsevier},
 title = {Where should I publish? Heterogeneous, networks-based prediction of paper’s citation success},
 venue = {Journal of Informetrics},
 volume = {15}
}

