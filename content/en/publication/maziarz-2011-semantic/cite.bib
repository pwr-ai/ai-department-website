 author = {Maziarz, Marek and Piasecki, Maciej and Szpakowicz, Stanisław and Rabiega-Wiśniewska, Joanna and Hojka, Bożena},
 journal = {Cognitive studies},
 number = {11},
 pub_year = {2011},
 publisher = {Instytut Slawistyki Polskiej Akademii Nauk},
 title = {Semantic relations between verbs in polish wordnet 2.0},
 venue = {Cognitive studies}
}

