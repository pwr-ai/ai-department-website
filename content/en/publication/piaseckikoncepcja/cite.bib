@article{piaseckikoncepcja,
 abstract = {Artykuł przedstawia koncepcje modelu definiującego struktury reprezentacji znaczenia dla polskich fraz nominalnych. Szczególną uwagę poświęcono zjawiskom: referencji, kwantyfikacji i generyczności. Prezentacja modelu została poprzedzona krótkim przeglądem wybranych prac dotyczących tych zjawisk. Omówione zostało też unifikujące podejście Hessa do referencji i kwantyfikacji. Prezentowany model opiera się na pojęciach: klasy i obiektu, kwantyfikatora uogólnionego oraz dynamicznego podejścia do znaczenia. Model},
 author = {Piasecki, Maciej},
 pub_year = {NA},
 title = {Koncepcja obiektowego modelu znaczenia polskiej frazy nominalnej.},
 venue = {NA}
}

