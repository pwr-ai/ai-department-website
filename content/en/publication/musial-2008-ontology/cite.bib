 author = {Musiał, Katarzyna and Juszczyszyn, Krzysztof and Kazienko, Przemysław},
 journal = {System Science},
 number = {1},
 pub_year = {2008},
 title = {Ontology-based recommendation in multimedia sharing systems},
 venue = {System Science},
 volume = {34}
}

