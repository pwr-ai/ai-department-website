@inproceedings{augustyniak2016fast,
 abstract = {A lexicon-based ensemble approach to sentiment analysis that outperforms lexicon-based method is presented in this article. This method consists of two steps. First we employ our own method (called frequentiment) for automatic generation of sentiment lexicons and some of publicly available lexicons. Secondly, an ensemble classification is used to improve the overall accuracy of predictions. Our approach outperforms publicly available sentiment lexicons and automatically generated domain lexicons. We conduct comprehensive analysis},
 author = {Augustyniak, Łukasz and Szyma\ŉski, Piotr and Kajdanowicz, Tomasz and Kazienko, Przemysław},
 booktitle = {Asian Conference on Intelligent Information and Database Systems},
 organization = {Springer},
 pages = {108--116},
 pub_year = {2016},
 title = {Fast and accurate-improving lexicon-based sentiment classification with an Ensemble Methods},
 venue = {Asian Conference on …}
}

