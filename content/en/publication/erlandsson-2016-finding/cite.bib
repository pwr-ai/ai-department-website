@article{erlandsson2016finding,
 abstract = {Influential users play an important role in online social networks since users tend to have an impact on one other. Therefore, the proposed work analyzes users and their behavior in order to identify influential users and predict user participation. Normally, the success of a social media site is dependent on the activity level of the participating users. For both online social networking sites and individual users, it is of interest to find out if a topic will be interesting or not. In this article, we propose association learning to detect relationships},
 author = {Erlandsson, Fredrik and Br√≥dka, Piotr and Borg, Anton and Johnson, Henric},
 journal = {Entropy},
 number = {5},
